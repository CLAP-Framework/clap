<launch>

  <machine name="miivii-tegra" address="192.168.110.125" user="nvidia" password="nvidia" env-loader="/opt/ros/melodic/miivii-tegra_env.sh" />
    <group ns="middle">

    <node machine="miivii-tegra" name="rslidar_node" pkg="rslidar_driver" type="rslidar_node" output="screen" >
      <param name="model" value="RS32"/>
      <param name="device_ip" value="192.168.110.200"/>
      <param name="msop_port" value="6600"/>
      <param name="difop_port" value="6601"/>
      <param name="time_synchronization" value="true"/>
    </node>
    </group>
    
  <group ns="middle">
    <arg name="lidar_model" default="RS32" />
    <arg name="lidar_param_path" default="$(find rslidar_pointcloud)/data/rs_lidar_32/" />

    <!-- <node  name="rslidar_node" pkg="rslidar_driver" type="rslidar_node" output="screen" >
      <param name="model" value="$(arg lidar_model)"/>
      <param name="device_ip" value="192.168.110.200"/>
      <param name="msop_port" value="6600"/>
      <param name="difop_port" value="6601"/>
      <param name="time_synchronization" value="true"/>
    </node> -->

    <node  name="cloud_node" pkg="rslidar_pointcloud" type="cloud_node" output="screen" >
      <param name="model" value="$(arg lidar_model)"/>
      <param name="curves_path" value="$(arg lidar_param_path)/curves.csv" />
      <param name="angle_path" value="$(arg lidar_param_path)/angle.csv" />
      <param name="channel_path" value="$(arg lidar_param_path)/ChannelNum.csv" />
      <param name="curves_rate_path" value="$(arg lidar_param_path)/CurveRate.csv" />
      <param name="start_angle" value="0" />
      <param name="end_angle" value="360" />
    </node>
  </group>
</launch>