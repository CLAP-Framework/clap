<launch>
  <node pkg="icvfollower" name="icv_carla_control"  type="icv_carla_control" output="screen">
  <!-- set test target speed km/h  -->
  <param name="~target_speedcarla" type="double" value="10"/>  

  <!-- set Vehiclemode 1 carla 2 mini_auto 3  xp_car  4 nova_car-->
  <param name="~VehicleMode_flag" type="int" value="1"/>

  <!-- use for generate Path -->
  <param name="~_R_test" type="double" value="30"/>
  <param name="~_delta_d" type="double" value="0.2"/>
  </node> 
</launch>
